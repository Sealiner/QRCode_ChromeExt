{  
  "name": "QRCode",
  "manifest_version": 2,
  "version": "0.0.0",  
  "description": "turn url to qrcode",
  "background": {
  	"scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "page_action": {  
    "default_icon": "icon.png" ,
    "default_title": "show qrcode",
    "default_popup": "popup.html"
  }
} 